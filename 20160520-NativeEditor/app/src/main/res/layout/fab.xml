            android:layout_alignRight="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

        <ImageButton
            android:id="@+id/fab_action_read_file1"
            style="@style/FloatingActionButton.Mini.ReadFile"
            android:src="@drawable/ai"
            android:layout_toLeftOf="@+id/navigate_to_next_page"
            android:layout_toStartOf="@+id/navigate_to_next_page"
            android:layout_alignBottom="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

