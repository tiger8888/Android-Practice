        <ImageButton
            android:id="@+id/fab_action_go_to_line"
            style="@style/FloatingActionButton.Mini.ReadFile"
            android:src="@drawable/go_to_line"
            android:layout_above="@+id/fab_action_move_left"
            android:layout_alignEnd="@+id/navigate_to_next_page"
            android:layout_alignRight="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

        <ImageButton
            android:id="@+id/fab_action_move_left"
            style="@style/FloatingActionButton.Mini.ReadFile"
            android:src="@drawable/move_left"
            android:layout_above="@+id/fab_action_move_right"
            android:layout_alignEnd="@+id/navigate_to_next_page"
            android:layout_alignRight="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

        <ImageButton
            android:id="@+id/fab_action_move_right"
