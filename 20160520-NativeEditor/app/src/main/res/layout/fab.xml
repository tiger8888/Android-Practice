            android:layout_alignRight="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

        <ImageButton
            android:id="@+id/fab_action_max_line_number"
            style="@style/FloatingActionButton.Mini.Feature"
            android:src="@drawable/max_line_number"
            android:layout_above="@+id/fab_action_line_highlight"
            android:layout_alignEnd="@+id/navigate_to_next_page"
            android:layout_alignRight="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

        <ImageButton
            android:id="@+id/fab_action_line_highlight"
