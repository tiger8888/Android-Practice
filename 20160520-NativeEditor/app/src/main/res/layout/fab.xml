        <ImageButton
            android:id="@+id/fab_action_readonly"
            style="@style/FloatingActionButton.Mini.Feature"
            android:src="@drawable/read_only"
            android:layout_toLeftOf="@+id/fab_action_read_file1"
            android:layout_toStartOf="@+id/fab_action_read_file1"
            android:layout_alignBottom="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

        <ImageButton
            android:id="@+id/fab_action_show_undomanager"
