            android:src="@drawable/move_right"
            android:layout_above="@+id/fab_action_undo"
            android:layout_alignEnd="@+id/navigate_to_next_page"
            android:layout_alignRight="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

        <ImageButton
            android:id="@+id/fab_action_undo"
            style="@style/FloatingActionButton.Mini.UndoRedo"
            android:src="@drawable/undo"
            android:layout_above="@+id/fab_action_redo"
            android:layout_alignEnd="@+id/navigate_to_next_page"
            android:layout_alignRight="@+id/navigate_to_next_page"
            android:contentDescription="@null" />

